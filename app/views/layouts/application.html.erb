<!DOCTYPE html>
<html>

<%= render 'layouts/header' %>

<body>
  
  <%= render 'layouts/nav' %>


  <% if notice %>
  <p id="notice" class="alert alert-primary"><%= notice %></p>
  <% end %>
  <% if alert %>
  <p id="alert" class="alert alert-warning"><%= alert %></p>
  <% end %>


  <div class="container-fluid main-container">
    <%= yield %>
  </div>

  <%= render 'layouts/footer' %>

</body>
</html>
